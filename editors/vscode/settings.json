{
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "[dotenv]": {
        "editor.formatOnSave": false
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[latex]": {
        "editor.wordWrap": "on"
    },
    "[markdown]": {
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
        "editor.tabSize": 3
    },
    "[python]": {
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.semanticHighlighting.enabled": false
    },
    "[snowflake-sql]": {
        "editor.defaultFormatter": "mtxr.sqltools"
    },
    "[sql]": {
        "editor.defaultFormatter": "mtxr.sqltools"
    },
    "[toml]": {
        "editor.defaultFormatter": "be5invis.toml"
    },
    "autoDocstring.docstringFormat": "google",
    "aws.codeWhisperer.importRecommendation": false,
    "aws.codeWhisperer.includeSuggestionsWithCodeReferences": false,
    "aws.codeWhisperer.shareCodeWhispererContentWithAWS": false,
    "aws.telemetry": false,
    "breadcrumbs.enabled": true,
    "cSpell.enableFiletypes": [
        "!git-commit",
        "codeowners",
        "dockerfile",
        "markdown_latex_combined"
    ],
    "cSpell.enabled": true,
    "cSpell.language": "en,en-US",
    "cSpell.spellCheckOnlyWorkspaceFiles": true,
    "cSpell.userWords": [
        "airflowignore",
        "astype",
        "autolog",
        "autouse",
        "cloudpickle",
        "CODEOWNERS",
        "dagrun",
        "Dask",
        "dataframe",
        "dataframes",
        "datapoint",
        "datapoints",
        "datascience",
        "directscope",
        "direnv",
        "dropna",
        "Dtype",
        "dtypes",
        "entrypoints",
        "etls",
        "fillna",
        "getenv",
        "iloc",
        "isin",
        "isna",
        "joblib",
        "loguru",
        "matplotlib",
        "mgmt",
        "mlflow",
        "mlruns",
        "nunique",
        "outter",
        "Pandera",
        "Pipenv",
        "Pipfile",
        "pyarrow",
        "pydantic",
        "pyplot",
        "pyproject",
        "quantile",
        "scikit",
        "shap",
        "sklearn",
        "sonarcloud",
        "timespec",
        "Traceback"
    ],
    "editor.acceptSuggestionOnEnter": "on",
    "editor.cursorBlinking": "phase",
    "editor.fontFamily": "MesloLGLDZ Nerd Font Mono",
    "editor.fontLigatures": false,
    "editor.fontSize": 12,
    "editor.formatOnSave": false,
    "editor.inlineSuggest.enabled": true,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        80,
        120
    ],
    "editor.smoothScrolling": true,
    "editor.snippetSuggestions": "top",
    "editor.suggest.preview": true,
    "editor.suggest.showMethods": true,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.tabSize": 4,
    "editor.trimAutoWhitespace": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 0,
    "extensions.autoUpdate": true,
    "extensions.ignoreRecommendations": false,
    "files.associations": {
        "**/*_vars/**/*.yml": "ansible",
        "**/*ansible*/**/*.yml": "ansible",
        "**/defaults/**/*": "ansible",
        "**/handler/*.yml": "ansible",
        "**/inventory/*/*": "ansible",
        "**/playbooks/**/*.yml": "ansible",
        "**/roles/**/*.yml": "ansible",
        "**/tasks/**/*.yml": "ansible",
        "**/vars/**/*.yml": "ansible",
        "*.hcl": "terraform",
        "*.hql": "sql",
        "*.j2": "jinja",
        "*.jsonl": "json",
        "*.tmpl": "gotemplate",
        "*.yml": "yaml",
        ".coveragerc": "toml"
    },
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        "**/.ammonite": true,
        "**/.bloop": true,
        "**/.metals": true
    },
    "git.autofetch": true,
    "git.confirmSync": false,
    "github.copilot.enable": {
        "*": true,
        "markdown": false,
        "plaintext": false,
        "scminput": false,
        "yaml": false
    },
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.recentChange.enabled": false,
    "gitlens.codeLens.scopes": [
        "document",
        "containers"
    ],
    "gitlens.hovers.annotations.over": "annotation",
    "gitlens.hovers.currentLine.changes": false,
    "gitlens.hovers.currentLine.enabled": false,
    "gitlens.views.commitDetails.files.layout": "tree",
    "gitlens.views.repositories.showBranches": false,
    "gitlens.views.repositories.showCommits": false,
    "gitlens.views.repositories.showContributors": false,
    "gitlens.views.repositories.showRemotes": false,
    "gitlens.views.repositories.showTags": false,
    "go.autocompleteUnimportedPackages": true,
    "go.buildFlags": [
        "-v"
    ],
    "go.coverOnSingleTest": true,
    "go.coverageDecorator": {
        "type": "gutter"
    },
    "go.formatTool": "goimports",
    "go.testTimeout": "3m",
    "go.toolsManagement.autoUpdate": true,
    "go.useCodeSnippetsOnFunctionSuggest": true,
    "go.useLanguageServer": true,
    "html.format.wrapLineLength": 0,
    "isort.args": [
        "-w",
        "120",
        "--profile",
        "black"
    ],
    "java.completion.enabled": true,
    "java.configuration.runtimes": [
        {
            "default": true,
            "name": "JavaSE-11",
            "path": "/usr/local/lib/jvm/java/openjdk-19"
        }
    ],
    "java.jdt.ls.java.home": "/usr/local/lib/jvm/java/openjdk-19",
    "javascript.format.placeOpenBraceOnNewLineForControlBlocks": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "json.schemas": [],
    "jupyter.askForKernelRestart": false,
    "jupyter.disableJupyterAutoStart": true,
    "jupyter.widgetScriptSources": [
        "jsdelivr.com",
        "unpkg.com"
    ],
    "latex-workshop.docker.enabled": true,
    "latex-workshop.intellisense.package.enabled": true,
    "latex-workshop.view.pdf.viewer": "tab",
    "markdown.extension.list.indentationSize": "inherit",
    "markdownlint.config": {
        "MD007": false,
        "MD024": false,
        "MD033": false
    },
    "metals.javaHome": "/usr/local/lib/jvm/java/openjdk-19",
    "mypy.runUsingActiveInterpreter": true,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "notebook.lineNumbers": "on",
    "notebook.output.textLineLimit": 500,
    "oneDarkPro.bold": true,
    "oneDarkPro.editorTheme": "One Dark Pro Flat",
    "oneDarkPro.italic": false,
    "plantuml.render": "PlantUMLServer",
    "python.analysis.completeFunctionParens": true,
    "python.analysis.logLevel": "Warning",
    "python.languageServer": "Pylance",
    "python.linting.flake8Args": [
        "--ignore=W503"
    ],
    "python.testing.autoTestDiscoverOnSaveEnabled": false,
    "python.testing.pytestEnabled": true,
    "redhat.telemetry.enabled": false,
    "rest-client.enableTelemetry": false,
    "search.exclude": {
        "**/bower_components": true,
        "**/node_modules": true,
        "/.venv": true,
        "/vendor": true
    },
    "search.showLineNumbers": true,
    "search.smartCase": true,
    "search.useGlobalIgnoreFiles": true,
    "security.workspace.trust.untrustedFiles": "open",
    "shellcheck.customArgs": [
        "-x"
    ],
    "sonarlint.connectedMode.connections.sonarcloud": [
        {
            "connectionId": "clarity-ai",
            "organizationKey": "clarity-ai"
        }
    ],
    "sonarlint.ls.javaHome": "/usr/local/lib/jvm/java/openjdk-19",
    "sqltools.format": {
        "reservedWordCase": "upper"
    },
    "sqltools.highlightQuery": false,
    "sqltools.showStatusbar": false,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.inheritEnv": false,
    "terminal.integrated.profiles.osx": {
        "bash": {
            "args": [
                "-l"
            ],
            "icon": "terminal-bash",
            "path": "bash"
        },
        "fish": {
            "args": [
                "-l"
            ],
            "path": "fish"
        },
        "pwsh": null,
        "tmux": null,
        "zsh": {
            "args": [
                "-l",
                "-i"
            ],
            "path": "zsh"
        }
    },
    "todo-tree.filtering.excludeGlobs": [
        "**/_build/**",
        "**/htmlcov/**"
    ],
    "todo-tree.general.statusBar": "total",
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.highlights.enabled": false,
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.newWindowDimensions": "maximized",
    "window.restoreWindows": "one",
    "window.title": "${activeEditorMedium}${separator}${rootName}",
    "workbench.colorTheme": "Catppuccin Macchiato",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.showIcons": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "*.pdf": "latex-workshop-pdf-hook",
        "git-rebase-todo": "gitlens.rebase"
    },
    "workbench.fontAliasing": "auto",
    "workbench.iconTheme": "material-icon-theme",
    "yaml.customTags": [
        "!And",
        "!If",
        "!Not",
        "!Equals",
        "!Or",
        "!FindInMap",
        "!Base64",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!Select",
        "!Split",
        "!Join",
        "!And sequence",
        "!If sequence",
        "!Not sequence",
        "!Equals sequence",
        "!Or sequence",
        "!FindInMap sequence",
        "!Join sequence",
        "!Sub sequence",
        "!ImportValue sequence",
        "!Select sequence",
        "!Split sequence",
        "!reference sequence"
    ],
    "yaml.format.singleQuote": true
}
