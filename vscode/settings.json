{
    "breadcrumbs.enabled": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": true
    },
    "editor.cursorBlinking": "phase",
    "editor.fontFamily": "Source Code Pro, Menlo, Monaco, 'Courier New', monospace",
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "none",
    "editor.rulers": [
        80,
        120
    ],
    "editor.smoothScrolling": true,
    "editor.tabCompletion": "on",
    "editor.tabSize": 4,
    "editor.trimAutoWhitespace": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 0,
    "extensions.autoUpdate": true,
    "extensions.ignoreRecommendations": false,
    "files.associations": {
        "**/defaults/**/*": "ansible",
        "**/tasks/**/*.yml": "ansible",
        "**/handler/*.yml": "ansible",
        "**/*_vars/**/*.yml": "ansible",
        "**/roles/**/*.yml": "ansible",
        "**/playbooks/**/*.yml": "ansible",
        "**/*ansible*/**/*.yml": "ansible",
        "**/vars/**/*.yml": "ansible",
        "**/inventory/*/*": "ansible",
        "*.yml": "yaml",
        "*.j2": "properties",
        "*.tmpl": "gotemplate"
    },
    "files.autoSave": "onFocusChange",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "go.alternateTools": {
        "go-langserver": "gopls"
    },
    "go.autocompleteUnimportedPackages": true,
    "go.buildFlags": [
        "-v"
    ],
    "go.coverOnSingleTest": true,
    "go.coverageDecorator": {
        "type": "gutter"
    },
    "go.formatTool": "goimports",
    "go.languageServerExperimentalFeatures": {
        "format": true,
        "autoComplete": true
    },
    "go.testTimeout": "3m",
    "go.useCodeSnippetsOnFunctionSuggest": true,
    "go.useLanguageServer": false,
    "hdinsight.disableOpenSurveyLink": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "latex-workshop.docker.enabled": true,
    "latex-workshop.view.pdf.viewer": "tab",
    "metals.javaHome": "/Library/Java/JavaVirtualMachines/adoptopenjdk-8.jdk/Contents/Home",
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "/vendor": true,
        "/.venv": true
    },
    "search.showLineNumbers": true,
    "search.smartCase": true,
    "search.useGlobalIgnoreFiles": true,
    "sync.gist": "d59eff8638859f4bf69c5f723017fc00",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.cursorBlinking": true,
    "terraform.indexing.enabled": false,
    "terraform.languageServer.enabled": true,
    "terraform.telemetry.enabled": false,
    "todohighlight.isEnable": true,
    "window.newWindowDimensions": "maximized",
    "window.restoreWindows": "none",
    "window.title": "${activeEditorMedium}${separator}${rootName}",
    "window.zoomLevel": 0,
    "workbench.activityBar.visible": true,
    "workbench.colorTheme": "Nord Wave",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.showIcons": true,
    "workbench.fontAliasing": "auto",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.startupEditor": "newUntitledFile",
    "yaml.customTags": [
        "!And",
        "!If",
        "!Not",
        "!Equals",
        "!Or",
        "!FindInMap",
        "!Base64",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!Select",
        "!Split",
        "!Join"
    ]
}
