{
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[latex]": {
        "editor.wordWrap": "on"
    },
    "[markdown]": {
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
    },
    "[python]": {
        "editor.semanticHighlighting.enabled": false
    },
    "[sql]": {
        "editor.defaultFormatter": "mtxr.sqltools"
    },
    "[toml]": {
        "editor.defaultFormatter": "be5invis.toml"
    },
    "autoDocstring.docstringFormat": "google",
    "aws.profile": "profile:mgmt",
    "aws.telemetry": false,
    "breadcrumbs.enabled": true,
    "editor.acceptSuggestionOnEnter": "on",
    "editor.codeActionsOnSave": {
        "source.organizeImports": true
    },
    "editor.cursorBlinking": "phase",
    "editor.fontFamily": "'MesloLGLDZ Nerd Font Mono'",
    "editor.fontLigatures": false,
    "editor.fontSize": 12,
    "editor.formatOnType": true,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        80,
        120
    ],
    "editor.smoothScrolling": true,
    "editor.snippetSuggestions": "top",
    "editor.suggest.preview": true,
    "editor.suggest.showMethods": true,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.tabSize": 4,
    "editor.trimAutoWhitespace": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 0,
    "extensions.autoUpdate": true,
    "extensions.ignoreRecommendations": false,
    "files.associations": {
        "**/*_vars/**/*.yml": "ansible",
        "**/*ansible*/**/*.yml": "ansible",
        "**/defaults/**/*": "ansible",
        "**/handler/*.yml": "ansible",
        "**/inventory/*/*": "ansible",
        "**/playbooks/**/*.yml": "ansible",
        "**/roles/**/*.yml": "ansible",
        "**/tasks/**/*.yml": "ansible",
        "**/vars/**/*.yml": "ansible",
        "*.hcl": "terraform",
        "*.hql": "sql",
        "*.j2": "jinja",
        "*.jsonl": "json",
        "*.tmpl": "gotemplate",
        "*.yml": "yaml",
        ".coveragerc": "toml"
    },
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        "**/.ammonite": true,
        "**/.bloop": true,
        "**/.metals": true
    },
    "git.confirmSync": false,
    "gitlens.views.branches.branches.layout": "list",
    "go.autocompleteUnimportedPackages": true,
    "go.buildFlags": [
        "-v"
    ],
    "go.coverOnSingleTest": true,
    "go.coverageDecorator": {
        "type": "gutter"
    },
    "go.formatTool": "goimports",
    "go.testTimeout": "3m",
    "go.toolsManagement.autoUpdate": true,
    "go.useCodeSnippetsOnFunctionSuggest": true,
    "go.useLanguageServer": true,
    "html.format.wrapLineLength": 0,
    "isort.args": [
        "-w",
        "120",
        "--profile",
        "black"
    ],
    "java.completion.enabled": true,
    "java.configuration.runtimes": [
        {
            "default": true,
            "name": "JavaSE-11",
            "path": "/usr/local/lib/jvm/java/openjdk-11"
        }
    ],
    "java.jdt.ls.java.home": "/usr/local/lib/jvm/java/openjdk-11",
    "javascript.format.placeOpenBraceOnNewLineForControlBlocks": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "json.schemas": [],
    "jupyter.askForKernelRestart": false,
    "jupyter.disableJupyterAutoStart": true,
    "latex-workshop.docker.enabled": true,
    "latex-workshop.intellisense.package.enabled": true,
    "latex-workshop.view.pdf.viewer": "tab",
    "markdownlint.config": {
        "MD007": false,
        "MD024": false,
        "MD033": false
    },
    "metals.javaHome": "/usr/local/lib/jvm/java/openjdk-11",
    "mypy.runUsingActiveInterpreter": true,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "notebook.lineNumbers": "on",
    "notebook.output.textLineLimit": 500,
    "oneDarkPro.bold": true,
    "oneDarkPro.editorTheme": "One Dark Pro Flat",
    "oneDarkPro.italic": false,
    "plantuml.render": "PlantUMLServer",
    "python.analysis.completeFunctionParens": true,
    "python.analysis.logLevel": "Warning",
    "python.formatting.provider": "black",
    "python.languageServer": "Pylance",
    "python.linting.flake8Args": [
        "--ignore=W503"
    ],
    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "python.testing.autoTestDiscoverOnSaveEnabled": false,
    "python.testing.pytestEnabled": true,
    "redhat.telemetry.enabled": false,
    "rest-client.enableTelemetry": false,
    "search.exclude": {
        "**/bower_components": true,
        "**/node_modules": true,
        "/.venv": true,
        "/vendor": true
    },
    "search.showLineNumbers": true,
    "search.smartCase": true,
    "search.useGlobalIgnoreFiles": true,
    "shellcheck.customArgs": [
        "-x"
    ],
    "sonarlint.ls.javaHome": "/usr/local/lib/jvm/java/openjdk-11",
    "sql-formatter.uppercase": true,
    "sqltools.format": {
        "reservedWordCase": "upper"
    },
    "sqltools.useNodeRuntime": true,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.profiles.osx": {
        "bash": {
            "args": [
                "-l"
            ],
            "icon": "terminal-bash",
            "path": "bash"
        },
        "fish": {
            "args": [
                "-l"
            ],
            "path": "fish"
        },
        "pwsh": null,
        "tmux": null,
        "zsh": {
            "args": [
                "-l",
                "-i"
            ],
            "path": "zsh"
        }
    },
    "terraform.languageServer": {
        "args": [
            "serve"
        ],
        "external": true
    },
    "todo-tree.filtering.excludeGlobs": [
        "**/_build/**",
        "**/htmlcov/**"
    ],
    "todo-tree.general.statusBar": "total",
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.highlights.enabled": false,
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.newWindowDimensions": "maximized",
    "window.restoreWindows": "one",
    "window.title": "${activeEditorMedium}${separator}${rootName}",
    "workbench.activityBar.visible": true,
    "workbench.colorTheme": "One Dark Pro Darker",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.showIcons": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.fontAliasing": "auto",
    "workbench.iconTheme": "material-icon-theme",
    "yaml.customTags": [
        "!And",
        "!If",
        "!Not",
        "!Equals",
        "!Or",
        "!FindInMap",
        "!Base64",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!Select",
        "!Split",
        "!Join",
        "!And sequence",
        "!If sequence",
        "!Not sequence",
        "!Equals sequence",
        "!Or sequence",
        "!FindInMap sequence",
        "!Join sequence",
        "!Sub sequence",
        "!ImportValue sequence",
        "!Select sequence",
        "!Split sequence"
    ]
}
